# Uncomment this line to define a global platform for your project
platform :ios, '10.0'

target 'YADL Reference App' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for YADL Reference App
  pod 'OMHClient', :git => 'https://github.com/ResearchSuite/OMHClient-ios'
  pod 'ResearchSuiteExtensions', :git => 'https://github.com/ResearchSuite/ResearchSuiteExtensions-iOS'
  pod 'ResearchSuiteResultsProcessor', :git => 'https://github.com/ResearchSuite/ResearchSuiteResultsProcessor-ios', :tag => '0.8.0'
  pod 'LS2SDK', :git => 'https://github.com/jdkizer9/LS2SDK-iOS', :tag => '0.5.1'
  pod "sdlrkx", :git => 'https://github.com/ResearchSuite/sdl-rkx'
  pod "ResearchKit"
  pod "ResearchSuiteTaskBuilder"
  pod "ResearchSuiteAppFramework"

    
    

  target 'YADL Reference AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
